env
# Network configurations
TESTNET_ENDPOINT=https://testnet.toncenter.com/api/v2/jsonRPC
TESTNET_API_KEY=your_testnet_api_key_here
MAINNET_ENDPOINT=https://toncenter.com/api/v2/jsonRPC
MAINNET_API_KEY=your_mainnet_api_key_here

# Contract addresses
TESTNET_CONTRACT_ADDRESS=your_testnet_contract_address
TESTNET_OWNER_WALLET=your_testnet_owner_wallet
MAINNET_CONTRACT_ADDRESS=your_mainnet_contract_address
MAINNET_OWNER_WALLET=your_mainnet_owner_wallet

# Transaction settings
TRANSACTION_TIMEOUT=60
RETRY_COUNT=3
MESSAGE_EXPIRATION=3600

# Gas configurations
DEFAULT_GAS_LIMIT=1000000
GAS_PRICE_FACTOR=1.5

# TON SDK settings
MESSAGE_PROCESSING_TIMEOUT=60
WAIT_FOR_TIMEOUT=60
OUT_OF_SYNC_THRESHOLD=15

# Game settings
MIN_DEPOSIT=0.1
MAX_DEPOSIT=100
CONVERSION_RATE=1